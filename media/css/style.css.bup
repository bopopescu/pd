/* *************************** 
   $5 Sidebar Styles
   ***************************/
#sidebar {
	width:300px;
	padding:10px 10px 10px 0;
	float:right;
}
#sidebar .schedule {
	width:250px;
	border:#e2d7ba solid 1px;
	padding:15px 24px;
	margin:0 0 15px;
	background:#FFF4D4;
	position:relative;
	color:#786532;
}
#sidebar .schedule .more {
	width:32px;
	height:13px;
	background:url(../images/bgnew.png) no-repeat;
	position:absolute;
	top:-1px;
	right:-1px;
}
#sidebar h3 {
	font-size:14px;
	padding:0 0 5px;
}
#sidebar p {
	padding:0 0 10px;
}
