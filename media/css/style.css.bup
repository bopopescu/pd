/* *************************** 
  Site - playdotion-web
  ---------------------------
  StyleSheet Index
  $1 - Global Styles
  $2 - Website Structure Styles
  $3 - Header Styles
  $4 - Content Styles
  $5 - Sidebar Styles
  $6 - Footer Styles
   ***************************/

/* *************************** 
   $1 GLOBAL Styles
   ***************************/



* {
	margin: 0;
	padding: 0;
}
a {
	color:#003e7e;
	text-decoration:none;
}
a:hover {
	color:#347307;
	text-decoration:none;
}
img {
	border:0;
	vertical-align:middle;
}
fieldset {
	border:none;
}
ul li {
	list-style:none;
}
/* *************************** 
   $2 Website Structure Styles
   ***************************/
html {
	background:#043351;
}
body {
	font-family:Arial, Helvetica, sans-serif;
	font-size:12px;
	color:#454545;
	background:#ffffff;
}
#container, #content {
	width:960px;
	margin:0 auto;
}
/* *************************** 
   $3 Header Styles
   ***************************/
#header {
	width:100%;
	height:100px;
	overflow:hidden;
}
#logo {
	width:200px;
	height:45px;
	margin:40px 0 0 0;
	float:left;
	padding:0 0 0 10px;
}
#nav {
	float:right;
	height:70px;
	margin:42px 0 0;
	padding:0 10px 0 0;
	background:url(../images/navbg.png) no-repeat 100% -76px;
	height:70px;
}
#nav .navleft {
	float:left;
	height:70px;
	overflow:hidden;
}
#nav ul {
	background:url(../images/navbg.png) no-repeat;
	height:38px;
	overflow:hidden;
	padding:0 0 0 10px;
	float:left;
}
#nav li {
	background:url(../images/navbg.png) repeat-x 0 -38px;
	float:left;
	height:27px;
	padding:11px 10px 0;
	font-weight:bold;
	font-size:14px;
}
#nav li a {
	color:#AA8111;
}
#nav li a:hover, #nav li.current a {
	color:#513B01;
}
#nav .navright {
	float:left;
}
#nav .navsearch {
	background:url(../images/navbg.png) repeat-x 0 -38px;
	height:30px;
	padding:8px 10px 0 0;
	overflow:hidden;
	float:left;
}
#nav .navsearch .innerns {
	padding:0 0 0 50px;
	float:left;
}
#nav .navsearch select {
	width:67px;
	margin:0;
}
#nav .navsearch .ipt {
	border-width:1px 1px 1px 0;
	border-color:#A77C09 #ECC353 #FFDD83;
	border-style:solid;
	margin:0 2px 0 -3px;
*margin-top:-1px;
	height:18px;
	padding:2px 5px 0;
	color:#000;
	font-size:12px;
	font-family:Arial, Helvetica, sans-serif;
	vertical-align:top;
}
#nav .navsearch .btn {
	background:#987106 url(../images/btngo.png) no-repeat;
	color:#FFEFC5;
	border:0 none;
	overflow:visible;
	font-family:Arial, Helvetica, sans-serif;
	width:28px;
	height:21px;
	padding:0;
	display:inline-block;
	vertical-align:top;
	cursor:pointer;
}
#nav .info {
	padding:3px 0 0;
	clear:both;
	font-size:11px;
}
#nav .info span {
	padding:0 5px;
	color:#247AB6;
}
#nav .info span a {
	color:#247AB6;
	font-weight:400;
}
#nav .info a {
	color:#73B11F;
	font-weight:bold;
}
#nav .login {
	clear:both;
	padding:3px 0 0;
	text-align:right;
	color:#A5A5A5;
	font-size:11px;
}
#nav .login a {
	color:#FB8D3A;
	font-weight:bold;
	margin:0 3px;
}
#nav .login a.logout {
	color:#A5A5A5;
	font-weight:normal;
}
.iDiv {
	position:absolute;
	height:22px;
	font-size:12px;
	color:#8B6601;
	line-height:22px;
	text-indent:4px;
	vertical-align:top;
	z-index:2;
	cursor:default;
	background:url(../images/selectbg.png) no-repeat right center;
}
.iDiv_over {
	color:#8B6601;
	background-position:right center;
}
.iDiv_out {
	color:#333;
	background-position:right center;
}
.cDiv {
	position:absolute;
	z-index:999;
	moz-user-select:none;
	border:1px solid #A77C09;
	cursor:default;
	background:#A77C09;
	overflow:auto;
	z-index:1;
}
.cDiv ul {
	font-size:15px;
	list-style:none;
	margin:0;
	padding:0;
}
.cDiv ul li {
	height:20px;
	line-height:20px;
	text-indent:4px;
}
.login_out {
	position:absolute;
	right:20px;
	top:46px;
	font-size:25px;
	font-weight:bold;
}
.login_out a {
	color:#2a72ca;
}
.login_out a:hover {
	text-decoration:underline;
}
/* *************************** 
   $4 Content Styles
   ***************************/

#content {
	overflow:hidden;
	padding:10px 0;
}
#content.add_padding {
	padding:27px 0 0;
}
.reg {
	width:100%;
	overflow:hidden;
	position:relative;
	border-bottom:1px solid #EAEAEA;
}
.regtop {
	background:url(../images/reg_star.png) no-repeat;
	padding:0 0 0 30px;
	height:47px;
	line-height:25px;
	margin:10px 0;
}
.regtop strong {
	color:#333333;
	font-size:25px;
	margin:0 10px 0 0;
	float:left;
}
.col_left .regtop span {
	font-size:12px;
	font-weight:normal;
	line-height:25px;
}
.regtop a, .regtop a:hover {
	color:#EA8639;
	font-size:14px;
}
.regform {
	width:455px;
	float:left;
}
.regform .innerform {
	width:440px;
	border:#EAEAEA solid 1px;
	background:#f5f5f5;
	padding:0 0 22px 15px;
	border-bottom:0;
	float:left;
}
.regimg {
	position:absolute;
	right:0;
	bottom:0;
	width:390px;
	height:497px;
	_height:504px;
	overflow:hidden;
	background:url(../images/regimg.png) no-repeat right bottom;
	float:right;
	margin:0 0 0 -10px;
}
@media screen and (-webkit-min-device-pixel-ratio:0) {
 .regimg {
height:502px;
}
}
.regform .ipt {
	color:#a4a4a4;
	font-size:16px;
	padding:8px 5px;
	border:#D0D0D0 solid 1px;
	background:#fefefe;
}
.form_wrapper .input_text.error_tip, .form_wrapper select.error_tip,.form_wrapper .error_tip{
	border:#ee0000 solid 1px;
	background:#fffbf0;
}
.nameinner {
	width:200px;
	padding:0 13px 0 0;
	float:left;
	position:relative;
}
.nameinner label {
	color:#4d4d4d;
	font-size:16px;
	font-weight:bold;
	padding:0 0 5px;
}
.nameinner .ipt {
	width:170px;
	margin:5px 8px 0 0;
}
.innerform ul {
	width:100%;
	padding:20px 0 0;
	float:left;
}
.innerform li {
	padding:0 0 15px;
	position:relative;
	width:100%;
	vertical-align:middle;
	float:left;
}
.innerform li label {
	color:#4d4d4d;
	font-size:16px;
	font-weight:bold;
	padding:0 0 5px;
	display:block;
}
.innerform li .ipt {
	width:365px;
	vertical-align:middle;
	margin:0 8px 0 0;
}
.innerform li .iptinfo {
	position:absolute;
	top:10px;
	right:70px;
	color:#a4a4a4;
	font-size:10px;
}
.errorinfo, .rightinfo {
	display:inline-block;
	vertical-align:middle;
	width:45px;
	height:35px;
	background:url(../images/iptinfo.png) no-repeat 0 0;
}
.errorinfo {
	background-position:0 100%;
}
.innerform li .zip {
	width:140px;
}
.regform .tips, .form_wrapper span.tips {
	position:absolute;
	top:-12px;
	padding:0 0 0 30px;
	height:42px;
	color:#FFF;
	float:left;
	display:block;
	background:url(../images/tips.png) no-repeat;
	z-index:9;
	white-space:nowrap;
}
.regform .tips b, .form_wrapper span.tips b {
	float:left;
	padding:8px 20px 7px 0;
	height:22px;
	line-height:22px;
	background:url(../images/tips.png) no-repeat right top;
	margin:0 -5px 0 0;
	position:relative;
}
.regbtn {
	clear:both;
	width:100%;
	padding:20px 0 0;
	overflow:hidden;
}
.regbtm {
	clear:both;
	width:100%;
	margin:0 auto;
	padding:20px 0 50px;
	overflow:hidden;
}
.regbtmleft {
	float:left;
	width:300px;
}
.common_wrapper .regbtmleft p {
	color:#a4a4a4;
	line-height:18px;
	font-size:11px;
	padding:0 0 20px;
}
.regbtmleft .btn {
	width:164px;
	height:42px;
	border:0 none;
	background:url(../images/regbtn.png) no-repeat;
	cursor:pointer;
}
.regbtmleft a {
	color:#597b95;
}
.regbtmright {
	width:340px;
	float:right;
	padding:60px 0 0;
}
.regbtm.account_form {
	padding:0 0 50px;
}
.regbtm.account_form .regbtmright {
	padding:0;
}
/* *************************** 
   $6 Footer Styles
   ***************************/
#footer {
	width:100%;
	height:84px;
	background:url(../images/ftbg.png) repeat-x;
	overflow:hidden;
}
.footer {
	width:960px;
	height:54px;
	background:url(../images/ftlogo.png) no-repeat;
	margin:31px auto;
}
.footer ul {
	float:left;
	padding:6px 0 0 140px;
	font-size:15px;
}
.footer li {
	float:left;
	padding:0 30px 0 0;
}
.footer a, .footer a:hover {
	color:#fff;
}
.footer .copy {
	color:#fff;
	font-size:9px;
	font-family:verdana;
	line-height:15px;
	float:left;
	padding:2px 0 0 5px;
}
.footer .copy a {
	font-weight:bold;
	text-decoration:none;
	color:#fff;
}
.footer .copy a:hover {
	font-weight:bold;
	text-decoration:underline;
	color:#fff;
}
