<htL>
<head>
<title>Twitter Feed</title>
</head>
<body>
<script src="http://widgets.twimg.com/j/2/widget.js"></script>
<script>
new TWTR.Widget({
  version: 2,
  type: 'list',
  rpp: 30,
  interval: 6000,
  title: 'Playdation tweets',
  subject: 'Playdation Bloggers',
  width: 175,
  height: 300,
  theme: {
    shell: {
      background: '#ffda74',
      color: '#ffffff'
    },
    tweets: {
      background: '#ffffff',
      color: '#444444',
      links: '#4051c2'
    }
  },
  features: {
    scrollbar: true,
    loop: false,
    live: true,
    hashtags: true,
    timestamp: true,
    avatars: true,
    behavior: 'all'
  }
}).render().setList('momblogmag', 'mom-blog-magazine-writers').start();
</script>        

</body>
</html>
